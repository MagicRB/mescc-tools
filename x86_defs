# 32bit mode instructions
DEFINE LOADI32_eax B8
DEFINE LOADI32_ebx BB
DEFINE LOADI32_ecx B9
DEFINE LOADI32_edx BA
DEFINE LOADI32_esi BE
DEFINE LOADI32_edi BF
DEFINE LOADI32_ebp BD
DEFINE LOADI32_esp BC

# 16bit realmode instructions
DEFINE LOADI16_ax B8
DEFINE LOADI16_bx BB
DEFINE LOADI16_cx B9
DEFINE LOADI16_dx BA
DEFINE LOADI16_si BE
DEFINE LOADI16_di BF
DEFINE LOADI16_bp BD
DEFINE LOADI16_sp BC

# Universal instructions
DEFINE LOADI8_ah B4
DEFINE LOADI8_al B0
DEFINE LOADI8_bh B7
DEFINE LOADI8_bl B3
DEFINE LOADI8_ch B5
DEFINE LOADI8_cl B1
DEFINE LOADI8_dh B6
DEFINE LOADI8_dl B2
